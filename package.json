{
  "version": "0.1.0",
  "name": "touch-biome",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "rimraf dist node_modules/.vite",
    "lint": "biome lint ./",
    "lint:fix": "biome lint --apply ./",
    "format": "biome format ./",
    "format:fix": "biome format --write ./",
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.4",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.77",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "vite-plugin-html": "^3.2.2"
  }
}
