{
  "version": "0.1.0",
  "name": "touch-biome",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "rimraf dist node_modules/.vite",
    "lint": "biome lint ./",
    "lint:fix": "biome lint --apply ./",
    "format": "biome format ./",
    "format:fix": "biome format --write ./",
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/node": "^20.9.5",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
    "vite-plugin-html": "^3.2.0"
  }
}
